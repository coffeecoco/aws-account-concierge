{
  "PolicyDocument": {
    "Version": "2012-10-17",
    "Statement": [ {
      "Sid": "CloudTrailCreateLogStream",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogStream"
      ],
      "Resource": [
        "arn:aws:logs:us-east-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:us-west-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:us-west-2:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:eu-west-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:eu-central-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:ap-southeast-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:ap-southeast-2:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:ap-northeast-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:sa-east-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*"
      ]
    },
    {
      "Sid": "CloudTrailPutLogEvents",
      "Effect": "Allow",
      "Action": [
        "logs:PutLogEvents"
      ],
      "Resource": [
        "arn:aws:logs:us-east-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:us-west-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:us-west-2:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:eu-west-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:eu-central-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:ap-southeast-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:ap-southeast-2:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:ap-northeast-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*",
        "arn:aws:logs:sa-heast-1:ACCOUNTNUMBER:log-group:CloudTrail/DefaultLogGroup:log-stream:*"
      ]
    } ]
  },
  "PolicyName": "CloudTrail_CloudWatchLogs_Role_Policy"
}
